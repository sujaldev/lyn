Animate {
  Circle my_circle {
    radius: 50,
    x: 100, y: 300
  }

  Rectangle my_rect {
    x: 500, y: 500,
    width: 200, height: 100, stroke_width: 10
  }

  Line my_line {
    x1: 700, y1: 20, x2: 100, y2: 700
  }

  play WriteLine {
    object: my_line
  }

  play FadeIn {
    object: my_rect
  }

  play Delay {
    duration: 2
  }

  play FadeIn {
    object: my_circle
  }

}
